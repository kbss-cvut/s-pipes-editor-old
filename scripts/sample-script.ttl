                                                                    sp:arg2  spin:_arg3 ;
                                                                    sp:arg3  0 ;
                                                                    sp:arg4  [ a        sp:sub ;
                                                                               sp:arg1  [ sp:varName  "rowCount" ] ;
                                                                               sp:arg2  1
                                                                             ] ;
                                                                    sp:arg5  [ sp:varName  "stepFunction" ] ;
                                                                    sp:arg6  [ sp:varName  "startValue" ]
                                                                  ] ;
                                                   sp:variable    [ sp:varName  "stepped" ]
                                                 ]
                                                 [ a              sp:Bind ;
