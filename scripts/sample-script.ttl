                                                   sp:subject    spin:_arg1
                                                 ]
                                                 [ a            sp:Optional ;
                                                   sp:elements  ( [ sp:object     [ sp:varName  "resultFunction" ] ;
                                                                    sp:predicate  spr:resultFunction ;
                                                                    sp:subject    spin:_arg1
                                                                  ]
                                                                )
