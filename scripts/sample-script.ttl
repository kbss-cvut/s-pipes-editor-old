sp:month  a              spin:Function ;
        rdfs:comment     "Extracts the month from a date/time literal." ;
        rdfs:label       "month" ;
        rdfs:subClassOf  spl:DateFunctions ;
        spin:constraint  [ a              spl:Argument ;
                           rdfs:comment   "The date or dateTime argument." ;
