                           spl:predicate  sml:view ;
                           spl:valueType  <http://uispin.org/ui#Node>
                         ] ;
        spin:constraint  [ a              spl:Argument ;
                           rdfs:comment   "True to only create a UISPIN snippet, e.g. without head/body." ;
                           spl:optional   true ;
